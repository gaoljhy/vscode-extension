{
	"name": "mytranslate",
	"displayName": "mytranslate",
	"description": "a vscode translate for few people",
	"version": "0.0.1",
	"publisher": "adman",
	"icon": "icon.jpeg",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.myTranslate"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.myTranslate",
				"title": "Translate"
			}
		],
		"editor/context": [
			{
				"when": "editorFocus",
				"command": "extension.myTranslate",
				"group": "navigation@6"
			}
		],
		"keybindings": [
			{
				"command": "extension.sayHello",
				"key": "ctrl+t",
				"mac": "cmd+t",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"vscode": "^1.1.36"
	}
}
